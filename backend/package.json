{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^22.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node build/index.js",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:migrate": "tsx src/scripts/runMigrations.ts",
    "benchmark:api": "tsx src/scripts/benchmarkApi.ts",
    "preserve:benchmark": "npm run build",
    "serve:benchmark": "node build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.25.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "nodemailer": "^7.0.11",
    "pdf-parse": "^2.4.5",
    "pg": "^8.16.3",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@rollup/rollup-darwin-arm64": "^4.53.5",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.9.0",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.15.6",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.5.14",
    "autocannon": "^8.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "supertest": "^7.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.44.0",
    "vitest": "^2.1.4"
  }
}
